UNIX = fnfs fnio fnnet fnsys fnsec fnzlib 
L1DEPS += x/lib9/lib9.a x/libflate/libflate.a x/libsec/libsec.a 

x/lib9/lib9.a:
	make -C x/lib9

x/libflate/libflate.a:
	make -C x/libflate

x/libsec/libsec.a:
	make -C x/libsec

L1EXTRAS := $(addsuffix .c, $(UNIX)) \
	     unix.c

L1FUNS   := $(UNIX)

TARG      := libl1.so
CFLAGS    += -fPIC -nostdlib

libl1.so: l1.o
	$(CC) -shared -Xlinker -Bsymbolic -o $@ $^
