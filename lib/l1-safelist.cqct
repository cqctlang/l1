@define listallocs(db)
{
	@local dom, lists;

	dom = db.exedom;
	lists = returns(db, db.interval(), &dom`_mklist);
	printf("%d calls to _mklist\n", length(lists));
	printf("return values are:\n");
	foreach(@lambda(it){ printf("\t%a (%t): %a\n", it, it.val, valpast(db, it.val, it.t)); }, lists);
}
