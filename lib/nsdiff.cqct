@include <nsutil.cqct>

if(length(args) != 3)
	error("usage: l1 nsdiff.l1 <rootns> <names1> <names2>");

rootns = args[0];
file1 = args[1];
file2 = args[2];

s = sprintfa("@names %s { @include <%s> };", rootns, file1);
ns1 = eval(s);
s = sprintfa("@names %s { @include <%s> };", rootns, file2);
ns2 = eval(s);

nsdiff(ns1, ns2);
