UNIX = fnfs fnio fnnet fnsys fnsec fnflate 
L1DEPS += x/lib9/lib9.a x/libflate/libflate.a x/libsec/libsec.a 

# expose CFLAGS to libraries
export CFLAGS

x/lib9/lib9.a:
	$(MAKE) -C x/lib9

x/libflate/libflate.a:
	$(MAKE) -C x/libflate

x/libsec/libsec.a:
	$(MAKE) -C x/libsec

L1EXTRAS := $(addsuffix .c, $(UNIX)) \
	     unix.c

L1FUNS   := $(UNIX)
