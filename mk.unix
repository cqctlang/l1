UNIX = fnfs fnio fnnet fnsys fnsec fnflate fnsnap
L1DEPS +=\
	x/libflate/libflate.a\
	x/libsec/libsec.a

x/libflate/libflate.a:
	$(MAKE) -s -C x/libflate

x/libsec/libsec.a:
	$(MAKE) -s -C x/libsec

x/libtre/libtre.a:
	$(MAKE) -s -C x/libtre

L1EXTRAS := $(addsuffix .c, $(UNIX)) \
	     unix.c

L1FUNS   := $(UNIX)
