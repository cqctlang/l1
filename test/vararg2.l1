printlist = lambda(l) {
	@local i;
	for(i = 0; i < length(l); i++)
		print(listref(l, i));
};

fn1 = lambda(arg ...) {
	@local x;
	print(length(arg));
	printlist(arg);
};

fn2 = lambda(arg ...) {
	print(length(arg));
	printlist(arg);
};

fn3 = lambda(arg ...) {
	@local x;
	x = arg;
	print(length(x));
	printlist(x);
};

fn1(1,2,3,4,5);
fn2(1,2,3,4,5);
fn3(1,2,3,4,5);
fn1();
fn2();
fn3();

