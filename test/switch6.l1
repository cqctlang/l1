@define test(x,y){
	printf("\t%d,%d\n", x, y);
	switch(x){
	case 1:
		switch(y){
		case 1:
			printf("case 1 -> case 1\n");
			break;
		case 2:
			printf("case 1 -> case 2\n");
			break;
		}
		break;
	case 2:
		switch(y){
		case 1:
			printf("case 2 -> case 1\n");
			break;
		case 2:
			printf("case 2 -> case 2\n");
			break;
		}
		break;
	}
}

test(1,1);
test(1,2);
test(2,1);
test(2,2);
test(0,0);
test(0,1);
test(0,2);
test(1,0);
test(2,0);
