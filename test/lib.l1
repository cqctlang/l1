
@include <library.cqct>

printf("Execution!\n");

@library(lib1,
  [ [f3, lib2.f1], [f4, lib2.f4] ],
  [f1, f2]) 
{ 
  @define f1() { printf("Running f1 from lib1\n"); }
  @define f2() { 
    f1();
    f3();
    f4();
    printf("Running f2 from lib1\n"); 
  }
}

printf("Execution!\n");

@library(lib2,
  [ [f2, lib1.f2] ],
  [f1, f4])
{
  @define f1() { printf("Running f1 from lib2\n"); }
  @define f4() { printf("Running f4 from lib2\n"); }
  f2();
}

