	@0x0	unsigned i;
};

fn = nsenumtype(ns);
vec = tabvals(fn(ns));
s = nil;
for(i = 0; i < veclen(vec); i++){
	s = vecref(vec, i);
	if(isstruct(s))
		break;
}

fs = fields(s);
for(i = 0; i < veclen(fs); i++){
	@local f, t;
	f = vecref(fs, i);
	t = fieldtype(f);
	if(isbitfield(t)){
		printf("bitfield %s (off = %d, width = %d, "
		       "pos = %d, container = %t)\n",
		       fieldid(f), fieldoff(f),
		       bitfieldwidth(t),
		       bitfieldpos(t),
		       bitfieldcontainer(t));
	}

}

